<h2>Sample data from the exercise table</h2>

<div class="row">
  <div class="col-4">
     {{#each exercises as |exercise| }}
          <ul class="list-group">
        <li class="list-group-item">
          <input type="checkbox" class="form-check-input me-1" value={{exercise.id}}>
          {{exercise.name}}
        </li>  
      </ul>
     {{/each}}
  </div>
</div>