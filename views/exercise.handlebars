<h2>Sample data from the exercise table for Arms</h2>

<div class="row">
  <div class="col-4">
     {{#each exercises as |exercise| }}
        <ul class="list-group">
          <li class="list-group-item">
            <input type="checkbox" class="form-check-input me-1" value={{id}}>
            {{name}}
          </li>  
      </ul>
     {{/each}}
  </div>
</div> 